{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "sort-imports": "warn",
        "eqeqeq": "warn",
        "max-len": "warn"
    }
}
